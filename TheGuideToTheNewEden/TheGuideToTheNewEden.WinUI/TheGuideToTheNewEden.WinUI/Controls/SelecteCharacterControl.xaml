<UserControl
    x:Class="TheGuideToTheNewEden.WinUI.Controls.SelecteCharacterControl"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="using:TheGuideToTheNewEden.WinUI.Controls"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">

    <ComboBox
        x:Name="CharacterComboBox"
        Margin="8,0,0,0"
        DisplayMemberPath="CharacterName"
        SelectionChanged="CharacterComboBox_SelectionChanged" />
</UserControl>
